import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;


public class Bot {

	public static void main(String[] args) throws Exception {
		
		int T, i, j, lenA, lenB;
		int[] arr1 = new int [1000003];
		int[] arr2 = new int [1000003];	
		String line;
		BufferedReader reader = new BufferedReader (new InputStreamReader(System.in));
		T = Integer.parseInt(reader.readLine());
		while(T != 0) {
			line = reader.readLine();
			String[] parts = line.split(" ");
			for (i=0; i<parts[0].length(); i++) {
				arr1[i] = 0;
			}
			for (i=0; i<parts[1].length(); i++) {
				arr2[i] = 0;
			}
			lenA = parts[0].length();
			lenB = parts[1].length();
			
			arr1[0] = 0;
			if (parts[0].charAt(0) == 'B')
				arr1[0] = 1;
			
			arr2[lenA-1] = 0;
			if (parts[0].charAt(lenA-1) == 'A')
				arr2[lenA-1] = 1;
			
			for (i=1; i<parts[0].length(); i++) {
				if (parts[0].charAt(i) == 'B') {
					arr1[i] = arr1[i-1] + 1;
				}
			}
			
			for (i=lenA-2; i>=0; i--) {
				if (parts[0].charAt(i) == 'A') {
					arr1[i] = arr1[i+1] + 1;
				}
			}
			parts[0] = parts[0].replace("#", "");
			parts[1] = parts[1].replace("#", "");
			if (parts[0].equals(parts[1])) {
				
				System.out.println("Yes");
			}
			else 
				System.out.println("No");
			T--;
		}

	}

}
